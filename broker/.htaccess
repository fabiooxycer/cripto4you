<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteCond %{HTTP_HOST} ^cripto4you.com [NC]
  RewriteRule ^(.*)$ https://cripto4you.com/$1 [L,R=301]

# PÁGINAS
  RewriteRule ^entrar/?$ index.php [NC,L]
  RewriteRule ^senha/?$ senha.php [NC,L]
  RewriteRule ^meu-perfil/?$ meu-perfil.php [NC,L]
  RewriteRule ^dashboard/?$ dashboard.php [NC,L]
  RewriteRule ^recupera-codigo/?$ recupera-codigo.php [NC,L]

# SITE
  RewriteRule ^noticias/?$ pages/site/noticias.php [NC,L]
  RewriteRule ^noticia-editar/?$ pages/site/noticia-editar.php [NC,L]

  # BROKER
  RewriteRule ^clientes/?$ pages/broker/clientes.php [NC,L]
  RewriteRule ^clientes-visulizar/?$ pages/broker/visualizar-editar.php [NC,L]
  RewriteRule ^clientes-editar/?$ pages/broker/clientes-editar.php [NC,L]

# SUPORTE
  RewriteRule ^fila-atendimento/?$ pages/suporte/suporte.php [NC,L]
  RewriteRule ^ver-chamado/?$ pages/suporte/ver-chamado.php [NC,L]

# INCLUDES
  RewriteRule ^sair/?$ includes/logout.php [NC,L]
  RewriteRule ^valida/?$ includes/validation.php [NC,L]

# DESATIVA CADASTROS
  RewriteRule ^ativa-cadastro/?$ pages/all/ativa-cadastro.php [NC,L]
  RewriteRule ^desativa-cadastro/?$ pages/all/desativa-cadastro.php [NC,L]
  RewriteRule ^desativa-franquia/?$ pages/all/desativa-franquia.php [NC,L]
  RewriteRule ^estatisticas-bancos/?$ pages/all/estatisticas-bancos.php [NC,L]
  RewriteRule ^ativa-pericia/?$ pages/all/ativa-pericia.php [NC,L]
  RewriteRule ^desativa-pericia/?$ pages/all/desativa-pericia.php [NC,L]

# TEMPLATE RESCISÃO BANCOS
  RewriteRule ^rescisao-contrato/?$ pages/rescisao/rescisao-contrato.php [NC,L]
  RewriteRule ^rescisao-bancos/?$ pages/rescisao/template_rescisao_bancos.php [NC,L]
  RewriteRule ^rescisao-digitalallocate/?$ pages/rescisao/template_rescisao_digitalallocate.php [NC,L]
  RewriteRule ^rescisao-digitalintelligentia/?$ pages/rescisao/template_rescisao_digitalintelligentia.php [NC,L]
  RewriteRule ^template-rescindir-contrato?$ pages/rescisao/template_rescisao_contrato.php [NC,L]
  RewriteRule ^reativa-contratos/?$ pages/rescisao/reativa-contratos.php [NC,L]
  RewriteRule ^template-reativa-contrato?$ pages/rescisao/template_reativa_contrato.php [NC,L]


</IfModule>

